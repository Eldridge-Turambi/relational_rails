<header>New Performers</header>
<%= form_with url: "/venues/#{@venue.id}/performers", method: :post, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :age %>
  <%= f.text_field :age %>

  <%= f.label :repeater %>
  <%= f.text_field :repeater %>

  <%= f.submit "Create New Performer" %>
<% end %>
