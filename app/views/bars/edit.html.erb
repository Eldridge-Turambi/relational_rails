<%= form_with url: "/bars/#{@bar.id}", method: :patch, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :employee_count %>
  <%= f.text_field :employee_count %>

  <%= f.label :license %>
  <%= f.text_field :license %>
  
  <%= f.submit "Update Bar" %>
<% end %>
