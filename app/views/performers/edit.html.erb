<%= form_with url: "/performers/#{@performer.id}", method: :patch, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :age %>
  <%= f.text_field :age %>

  <%= f.label :repeater %>
  <%= f.text_field :repeater %>
  <%= f.submit "Update This Performer" %>
<% end %>
